<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical"
	android:gravity="center" android:layout_width="match_parent"
	android:layout_height="wrap_content" xmlns:android="http://schemas.android.com/apk/res/android">
	<ImageView android:layout_width="wrap_content" android:id="@+id/iv_other_weather"
		android:layout_height="wrap_content"></ImageView>
	<TextView android:id="@+id/tv_ohter_week" android:textColor="#030303"
		android:layout_width="wrap_content" android:layout_height="wrap_content" />
	<TextView android:id="@+id/tv_ohter_temperature"
		android:textColor="#030303" android:layout_width="wrap_content"
		android:layout_height="wrap_content" />

</LinearLayout>

